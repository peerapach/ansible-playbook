client
dev tun
proto tcp
remote 103.212.36.132  443
resolv-retry infinite
nobind
user nobody
group nogroup
persist-key
persist-tun
comp-lzo
verb 3
cipher AES-128-CBC
auth SHA256
key-direction 1
auth-user-pass

<ca>
{{ ca_cert.content | b64decode }}
</ca>

<key>
{{ client_key.content | b64decode }}
</key>

<cert>
{{ client_cert.content | b64decode }}
</cert>
